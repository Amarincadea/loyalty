<?php

namespace Login\LoginBundle\Entity;

/**
 * Designation
 */
class Designation
{
    /**
     * @var string
     */
    private $designation;

    /**
     * @var string
     */
    private $base;

    /**
     * @var integer
     */
    private $dId;


    /**
     * Set designation
     *
     * @param string $designation
     *
     * @return Designation
     */
    public function setDesignation($designation)
    {
        $this->designation = $designation;

        return $this;
    }

    /**
     * Get designation
     *
     * @return string
     */
    public function getDesignation()
    {
        return $this->designation;
    }

    /**
     * Set base
     *
     * @param string $base
     *
     * @return Designation
     */
    public function setBase($base)
    {
        $this->base = $base;

        return $this;
    }

    /**
     * Get base
     *
     * @return string
     */
    public function getBase()
    {
        return $this->base;
    }

    /**
     * Get dId
     *
     * @return integer
     */
    public function getDId()
    {
        return $this->dId;
    }
}
