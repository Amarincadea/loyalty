<?php

namespace Login\LoginBundle\Entity;

/**
 * EmpWdReq
 */
class EmpWdReq
{
    /**
     * @var string
     */
    private $eid;

    /**
     * @var integer
     */
    private $percentage;

    /**
     * @var string
     */
    private $indecimal;

    /**
     * @var \DateTime
     */
    private $reqDate = 'CURRENT_TIMESTAMP';

    /**
     * @var \DateTime
     */
    private $whenRequred;

    /**
     * @var string
     */
    private $remark;

    /**
     * @var integer
     */
    private $wdid;


    /**
     * Set eid
     *
     * @param string $eid
     *
     * @return EmpWdReq
     */
    public function setEid($eid)
    {
        $this->eid = $eid;

        return $this;
    }

    /**
     * Get eid
     *
     * @return string
     */
    public function getEid()
    {
        return $this->eid;
    }

    /**
     * Set percentage
     *
     * @param integer $percentage
     *
     * @return EmpWdReq
     */
    public function setPercentage($percentage)
    {
        $this->percentage = $percentage;

        return $this;
    }

    /**
     * Get percentage
     *
     * @return integer
     */
    public function getPercentage()
    {
        return $this->percentage;
    }

    /**
     * Set indecimal
     *
     * @param string $indecimal
     *
     * @return EmpWdReq
     */
    public function setIndecimal($indecimal)
    {
        $this->indecimal = $indecimal;

        return $this;
    }

    /**
     * Get indecimal
     *
     * @return string
     */
    public function getIndecimal()
    {
        return $this->indecimal;
    }

    /**
     * Set reqDate
     *
     * @param \DateTime $reqDate
     *
     * @return EmpWdReq
     */
    public function setReqDate($reqDate)
    {
        $this->reqDate = $reqDate;

        return $this;
    }

    /**
     * Get reqDate
     *
     * @return \DateTime
     */
    public function getReqDate()
    {
        return $this->reqDate;
    }

    /**
     * Set whenRequred
     *
     * @param \DateTime $whenRequred
     *
     * @return EmpWdReq
     */
    public function setWhenRequred($whenRequred)
    {
        $this->whenRequred = $whenRequred;

        return $this;
    }

    /**
     * Get whenRequred
     *
     * @return \DateTime
     */
    public function getWhenRequred()
    {
        return $this->whenRequred;
    }

    /**
     * Set remark
     *
     * @param string $remark
     *
     * @return EmpWdReq
     */
    public function setRemark($remark)
    {
        $this->remark = $remark;

        return $this;
    }

    /**
     * Get remark
     *
     * @return string
     */
    public function getRemark()
    {
        return $this->remark;
    }

    /**
     * Get wdid
     *
     * @return integer
     */
    public function getWdid()
    {
        return $this->wdid;
    }
}
