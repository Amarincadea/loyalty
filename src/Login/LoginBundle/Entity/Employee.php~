<?php

namespace Login\LoginBundle\Entity;

/**
 * Employee
 */
class Employee
{
    /**
     * @var string
     */
    private $eid;

    /**
     * @var string
     */
    private $ename;

    /**
     * @var \DateTime
     */
    private $doj;

    /**
     * @var string
     */
    private $email;

    /**
     * @var string
     */
    private $edesignation;

    /**
     * @var \DateTime
     */
    private $edate = 'CURRENT_TIMESTAMP';

    /**
     * @var integer
     */
    private $id;


    /**
     * Set eid
     *
     * @param string $eid
     *
     * @return Employee
     */
    public function setEid($eid)
    {
        $this->eid = $eid;

        return $this;
    }

    /**
     * Get eid
     *
     * @return string
     */
    public function getEid()
    {
        return $this->eid;
    }

    /**
     * Set ename
     *
     * @param string $ename
     *
     * @return Employee
     */
    public function setEname($ename)
    {
        $this->ename = $ename;

        return $this;
    }

    /**
     * Get ename
     *
     * @return string
     */
    public function getEname()
    {
        return $this->ename;
    }

    /**
     * Set doj
     *
     * @param \DateTime $doj
     *
     * @return Employee
     */
    public function setDoj($doj)
    {
        $this->doj = $doj;

        return $this;
    }

    /**
     * Get doj
     *
     * @return \DateTime
     */
    public function getDoj()
    {
        return $this->doj;
    }

    /**
     * Set email
     *
     * @param string $email
     *
     * @return Employee
     */
    public function setEmail($email)
    {
        $this->email = $email;

        return $this;
    }

    /**
     * Get email
     *
     * @return string
     */
    public function getEmail()
    {
        return $this->email;
    }

    /**
     * Set edesignation
     *
     * @param string $edesignation
     *
     * @return Employee
     */
    public function setEdesignation($edesignation)
    {
        $this->edesignation = $edesignation;

        return $this;
    }

    /**
     * Get edesignation
     *
     * @return string
     */
    public function getEdesignation()
    {
        return $this->edesignation;
    }

    /**
     * Set edate
     *
     * @param \DateTime $edate
     *
     * @return Employee
     */
    public function setEdate($edate)
    {
        $this->edate = $edate;

        return $this;
    }

    /**
     * Get edate
     *
     * @return \DateTime
     */
    public function getEdate()
    {
        return $this->edate;
    }

    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }
}
